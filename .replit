modules = ["nodejs-20", "python-3.11"]
[agent]
expertMode = true
integrations = ["javascript_openai:1.0.0", "discord:1.0.0"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Discord Bot"

[[workflows.workflow]]
name = "Discord Bot"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node src/index.js"

[[workflows.workflow]]
name = "Discord Bot Debug"
author = 50255985
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node src/index.js"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5001
externalPort = 3000

[deployment]
deploymentTarget = "vm"
